package ie.gmit.sw;
/** @author Timothy Cassidy
 * The shingle class takes in a document id and
 * a hashCode.
 */
public class Shingle {
	// Member variable(s)
	private int docId;
	private int hashcode;
	
	/**
	 * Default constructor
	 */
	Shingle(){	
	}// Shingle(Default Constructor)
	
	/**
	 * Parameterised constructor
	 * @param docId int
	 * @param hashCode int
	 */
	Shingle(int docId, int hashCode){
		this.docId = docId;
		this.hashcode = hashCode;
	}// Shingle(Constructor)
	
	/**
	 * Getters and setters for the Shingle member variables.
	 * @return
	 */
	// Getters and Setters
	public int getDocId() {
		return docId;
	}// getDocId
	public void setDocId(int docId) {
		this.docId = docId;
	}// setDocId
	public int getHashcode() {
		return hashcode;
	}// getHashcode
	public void setHashcode(int hashcode) {
		this.hashcode = hashcode;
	}// setHashcode
	
}// Shingle